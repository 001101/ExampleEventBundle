{
    "name": "sulu/example-event-bundle",
    "type": "symfony-bundle",
    "keywords": [
        "sulu",
        "event",
        "tutorial"
    ],
    "homepage": "https://sulu.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Sulu Community",
            "homepage": "http://github.com/sulu/ExampleEventBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3",

        "doctrine/orm": "^2.6",
        "friendsofsymfony/rest-bundle": "^2.3",
        "jms/serializer-bundle": "^2.4",
        "league/tactician": "^1.0",
        "ramsey/uuid": "^3.7",
        "sulu/sulu": "^2.0@dev",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/http-foundation": "^3.4",
        "symfony/http-kernel": "^3.4",
        "webmozart/assert": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "jackalope/jackalope-doctrine-dbal": "^1.2.5",
        "jangregor/phpstan-prophecy": "^0.1.1",
        "phpstan/phpstan": "^0.9.2",
        "phpunit/phpunit": "^7.2",
        "symfony/browser-kit": "^3.4",
        "symfony/monolog-bundle": "^3.3",
        "zendframework/zend-stdlib": "~2.3",
        "zendframework/zendsearch": "@dev"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sulu\\Bundle\\ExampleEventBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
