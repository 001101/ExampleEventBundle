<?xml version="1.0" encoding="UTF-8"?>
<serializer>
    <class name="Sulu\Bundle\ExampleEventBundle\Model\Event" exclusion-policy="ALL">
        <virtual-property method="getId" serialized-name="id" expose="true"/>
        <virtual-property method="getTitle" serialized-name="title" expose="true"/>
        <virtual-property method="getDescription" serialized-name="description" expose="true"/>
        <virtual-property method="getStartDate" serialized-name="startDate" expose="true">
            <type><![CDATA[DateTime<'Y-m-d'>]]></type>
        </virtual-property>
        <virtual-property method="getEndDate" serialized-name="endDate" expose="true">
            <type><![CDATA[DateTime<'Y-m-d'>]]></type>
        </virtual-property>
    </class>
</serializer>
